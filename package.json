{
  "name": "bazy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "prepare": "husky install",
    "migrate-tool": "node src/utils/migrate.cjs",
    "make-migration": "run-s \"migrate-tool create -j sql {1}\" --",
    "make-migration:ts": "run-s \"migrate-tool create -j ts {1}\" --",
    "migrate": "run-s migrate:up",
    "migrate:up": "run-s \"migrate-tool up {1}\" -- ",
    "migrate:down": "run-s \"migrate-tool down {1}\" --",
    "migrate:redo": "run-s \"migrate-tool redo {1}\" --"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "ts-node": {
    "compilerOptions": {
      "module": "commonjs"
    }
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@faker-js/faker": "^7.6.0",
