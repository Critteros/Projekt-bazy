    "@mui/material": "^5.11.4",
    "@mui/x-data-grid": "^5.17.18",
    "@mui/x-date-pickers": "^5.0.13",
    "@next/bundle-analyzer": "^13.1.2",
    "@tanstack/react-query": "^4.20.0",
    "@trpc/client": "^10.8.1",
    "@trpc/next": "^10.8.1",
    "@trpc/react-query": "^10.8.1",
    "@trpc/server": "^10.8.1",
    "@types/bcrypt": "^5.0.0",
    "@types/ms": "^0.7.31",
    "bcrypt": "^5.1.0",
    "cookies-next": "^2.1.1",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "formik": "^2.2.9",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "ms": "^2.1.3",
    "next": "13.1.1",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
